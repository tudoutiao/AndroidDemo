<?xml version="1.0" encoding="utf-8"?>
<com.example.app.swip.SwipeMenuLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipe_menu_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#e9e9e9"
    app:isClickMenuAndClose="false"
    app:isEnableLeftMenu="false"
    app:isEnableSwipe="true"
    app:isOpenChoke="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"></TextView>

        <com.example.app.recycle.MyImageView
            android:id="@+id/icon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentRight="true"
            android:src="@mipmap/tab_my"></com.example.app.recycle.MyImageView>
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_menu1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="#fff"
        android:gravity="center"
        android:text="取消关注" />

</com.example.app.swip.SwipeMenuLayout>
